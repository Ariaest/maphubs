{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "browsers": [
              "> 5%",
              "not ie <= 11"
            ],
            "node": true
          }
        }
      }
    ],
    "@babel/preset-flow"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": true }],
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    "@babel/plugin-proposal-export-default-from"
  ],
  "ignore": [
    "/assets.*|node_modules/(?!(medium-editor|mapbox-gl|@google/earthengine)).*/"
  ]
}
