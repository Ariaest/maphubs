{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "browsers": ["> 5%", "not ie <= 11"],
            "node": true
          }
        }
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    "transform-flow-strip-types", "version-inline"
  ],
  "ignore": "/assets.*|node_modules\/(?!(medium-editor|mapbox-gl)).*/"
}